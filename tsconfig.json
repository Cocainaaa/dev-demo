{
  "compilerOptions": {
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata" : true,
    "skipLibCheck": true,
    "target": "es2016", 
    "outDir": "./dist",  
    "strict": true,  
    "forceConsistentCasingInFileNames": true,  
    "esModuleInterop": true,   
    "module": "commonjs",
  }
}
